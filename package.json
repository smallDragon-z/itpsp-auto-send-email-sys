{
  "name": "itpsp-auto-send-email-sys",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "next:lint": "next lint",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx ./src --report-unused-disable-directives --max-warnings 0 --fix",
    "lint-staged": "lint-staged",
    "lint:style": "stylelint \"src/**/*.{css,scss}\" --fix",
    "type-check": "tsc --noEmit -p tsconfig.json --composite false",
    "format": "prettier --write src/",
    "check": "npm run type-check && npm run lint && npm run lint:style",
    "commit": "git-cz",
    "commitlint": "commitlint --edit",
    "prepare": "husky install"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,tsx,ts,css,scss,md,json}": "prettier --ignore-unknown --write",
    "**/*.{css,scss}": "stylelint",
    "**/*.{js,jsx,ts,tsx}": "eslint . --ext .js,.jsx,.ts,.tsx ./src --report-unused-disable-directives --max-warnings 0"
  },
  "dependencies": {
    "@ant-design/cssinjs": "^1.18.4",
    "@ant-design/icons": "^5.2.6",
    "antd": "^5.13.2",
    "axios": "^1.6.5",
    "cheerio": "1.0.0-rc.12",
    "echarts": "^5.4.3",
    "echarts-for-react": "^3.0.2",
    "immer": "^10.0.3",
    "lodash": "^4.17.21",
    "next": "14.1.0",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.4",
    "@commitlint/config-conventional": "^18.4.4",
    "@commitlint/cz-commitlint": "^18.4.4",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "commitizen": "^4.3.0",
    "commitlint-config-cz": "^0.13.3",
    "cz-customizable": "^7.0.0",
    "cz-git": "^1.8.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.4",
    "stylelint": "^16.2.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recommended-scss": "^14.0.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-order": "^6.0.4",
    "stylelint-prettier": "^5.0.0",
    "typescript": "^5"
  }
}
